{
  "name": "@atscm/server-scripts",
  "version": "0.2.3",
  "description": "An atSCM project to create the server scripts required in atSCM",
  "main": "out/exports.js",
  "scripts": {
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "lint": "eslint Atviseproject.babel.js atscm src test",
    "test": "npm-run-all test:*",
    "test:unit": "ava test/src",
    "test:integration": "ava test/integration"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atSCM/server-scripts.git"
  },
  "keywords": [
    "atscm",
    "server",
    "scripts"
  ],
  "author": "Lukas Hechenberger <lukas.hechenberger@bachmann.info>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/atSCM/server-scripts/issues"
  },
  "homepage": "https://github.com/atSCM/server-scripts#readme",
  "devDependencies": {
    "@commitlint/cli": "6.2.0",
    "@commitlint/config-conventional": "6.1.3",
    "@ls-age/bump-version": "0.1.4",
    "@ls-age/eslint-config": "0.8.0",
    "atscm": "0.6.0-beta.12",
    "atscm-cli": "0.5.0-beta.0",
    "ava": "0.25.0",
    "babel-preset-env": "1.7.0",
    "babel-register": "6.26.0",
    "chalk": "2.3.2",
    "eslint": "4.19.0",
    "fs-extra": "5.0.0",
    "gulplog": "1.0.0",
    "husky": "0.14.3",
    "node-opcua": "0.0.60",
    "npm-run-all": "4.1.2",
    "rollup": "0.57.1",
    "rollup-plugin-babel": "3.0.3",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-node-resolve": "3.4.0",
    "tap-xunit": "2.2.0",
    "through2": "2.0.5"
  },
  "ava": {
    "require": [
      "babel-register",
      "./test/_setup.js"
    ]
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "semanticCommits": false,
    "commitMessagePrefix": "chore(package):",
    "commitMessageTopic": "{{depName}}",
    "packageRules": [
      {
        "packagePatterns": [ "rollup" ],
        "groupName": "rollup"
      }
    ]
  }
}
