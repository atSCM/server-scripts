{
  "name": "@atscm/server-scripts",
  "version": "0.0.0",
  "description": "An atSCM project to create the server scripts required in atSCM",
  "main": "out/exports.js",
  "scripts": {
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "lint": "eslint Atviseproject.babel.js atscm src test",
    "test": "npm-run-all test:*",
    "test:unit": "ava test/src",
    "test:integration": "ava test/integration"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LukasHechenberger/server-scripts.git"
  },
  "keywords": [
    "atscm",
    "server",
    "scripts"
  ],
  "author": "Lukas Hechenberger <lukas.hechenberger@bachmann.info>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LukasHechenberger/server-scripts/issues"
  },
  "homepage": "https://github.com/LukasHechenberger/server-scripts#readme",
  "devDependencies": {
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "@ls-age/babel-preset": "^0.4.0",
    "@ls-age/eslint-config": "^0.8.0",
    "atscm": "^0.6.0-beta.12",
    "ava": "^0.25.0",
    "babel-register": "^6.26.0",
    "eslint": "^4.19.0",
    "husky": "^0.14.3",
    "npm-run-all": "^4.1.2",
    "tap-xunit": "^2.2.0"
  },
  "ava": {
    "require": [
      "babel-register",
      "./test/_setup.js"
    ]
  }
}
